.profile-view
  padding: 150px 100px 100px
  background: #123d60
  
.profile-square
  border: 4px solid #333
  display: block
  max-width: 1000px
  height: 700px
  background: #37374d
  position: relative
  outline: 4px solid red
    
  .inner-border
    position: absolute
    width: 100%
    height: 100%
    border: 4px solid green
    display: block
    top: 0
    left: 0
    
  &:before
    content: ''
    position: absolute
    left: 5%
    top: -100px
    height: 200px
    width: 200px
    border: 10px solid #000
    border-radius: 50%
    z-index: 1
    background: #fff
